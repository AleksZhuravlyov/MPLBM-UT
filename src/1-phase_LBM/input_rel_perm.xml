<?xml version="1.0" ?>

<geometry>
    <!-- The name of the .dat file that contains the entire geometry-->
    <file_geom> spheres_equal_ </file_geom>
    <!-- Size (in voxels) of the simulation domain. -->
    <size> <x> 125 </x> <y> 120 </y> <z> 120 </z> </size>
</geometry>

<folder>
    <!-- Output folder -->
    <out_f> tester/ </out_f>
    <!-- Input folder -->
    <in_f> tester/ </in_f>
</folder>

<simulations>
	<!-- Total number of geometries / individual simulations -->
	<num> 37 </num>
	<!-- Pressure-->
    <press> 0.0005 </press>
	<!-- Max iterations-->
    <iter> 800 </iter>
</simulations>

